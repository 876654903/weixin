/* pages/tiket-Success/tiket-Success.wxss */
/**index.wxss**/

page{
  width:100%;
  height:100%;
  margin:0;
  padding:0;
  box-sizing: border-box;
  
}
.container{
  width:100%;
  height:100%;
  background: #F9F8FE;
  position: relative;
}
.container-top{
  width:100%;
  height:100%;
  position: relative;
}
.container-top .big-image{
  width:100%;
  height:100%;
}
.container-banner{
  position: absolute;
  top:25px;
  left:26px;
}
.container-banner image{
  width:360px;
  height:200px;
}

.container-content{
  position: absolute;
  top:248px;
  left:145px;
  
  
}
.container-content .content-title{
  width: 100%;
  display: flex;
}
.container-content .content-title .tit-img{
   width:34rpx;
   height:38rpx;
   /* background: #349AFF; */
   color:#3E9FFF;
  
}
.container-content .content-title .service{
  margin-left:7px;
  margin-top:-5px;
  font-weight: bold;
}
.content-list{
  position: absolute;
  left:-128px;
  top:-234px;
  width: 690rpx;
  height:622rpx;
  background: #F9F8FE;
  box-shadow: 0 8px 10px 2px rgba(210,227,235,0.50);
  border-radius: 10px;
  display: flex;
  flex-wrap:wrap;
  padding:10rpx 0;
  padding-top:30rpx;
}
.content-list .group{
  width: 100%;
  text-align: center;
  margin:60rpx 10rpx;
  border-bottom: 1px solid #E1E1E1;
}
.tit{
  font-family: PingFangSC-Medium;
  font-size: 16px;
  color: #2A2140;
  letter-spacing: -0.35px;
}
.send-wx{
  font-size: 14px;
  color: #C2C2C2;
}
.tiket-bottom{
   /* padding-left:85rpx; */
   width: 100%;
   text-align: center;
}
.go-forward{
  font-size: 14px;
  color: #2A2140;
}
button.returnContract{
  background: #349BFF;
  border-radius: 100px;
  font-size: 14px;
  color: #FFFFFF;
  margin-top:42rpx;
  width:558rpx;
  height:64rpx;
}

/* pages/invoiceError/invoiceError.wxss */
page,.invoiceError{
  width:100%;
  height:100%;
}
.invoiceError{
   position: relative; 
   padding:30rpx 0 0 0;
   box-sizing:border-box;
   background: #F9F8FE;
}

.imgss{
  position: absolute; 
  z-index:1;
  left:0;
  top:0;

}

.invoiceError_div{
  position: absolute; 
  z-index:2;
  width:85%;
  height:690rpx;
  background: #fff;
  left:50%;
  top:10;
   transform: translate(-50%,0); 
  border-radius: 10rpx;
  box-shadow: 0 8px 10px 2px rgba(210,227,235,0.50);
  text-align: center;
}
.Error{
  width:430rpx;
  height:450rpx;
  margin-top:90rpx;
}

.btn {
  background: #349BFF;
  border-radius: 100rpx;
  width:558rpx;
  height:64rpx;
  margin:0 auto;
  color:#fff;
}
